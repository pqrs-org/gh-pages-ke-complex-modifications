{
  "title": "Dual-purpose keys to relieve stress from your pinkies",
  "maintainers": [
    "marlonrichert"
  ],
  "rules": [
    {
      "description": "Dual Keys",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_shift"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 300,
            "basic.to_if_held_down_threshold_milliseconds": 0
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "hyphen"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "right_shift"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 300,
            "basic.to_if_held_down_threshold_milliseconds": 0
          }
        },
        {
          "type": "basic",
